[
  {
    "name": "data_loadcsv",
    "module": "data_loadcsv",
    "input": {
      "filename": "train.csv"
    },
    "options": {
      "column_header": true,
      "delim_whitespace": false
    }
  },
  {
    "name": "data_getxy",
    "module": "data_getxy",
    "input": {
      "dataframe": "output->data_loadcsv"
    },
    "options": {
      "xcols": [
        "crim",
        "zn",
        "indus",
        "chas",
        "nox",
        "rm",
        "age",
        "dis",
        "rad",
        "tax",
        "ptratio"
      ],
      "ycols": [
        "medv"
      ]
    }
  },
  {
    "name": "data_preprocess",
    "module": "data_preprocess",
    "method": "StandardScaler",
    "input": {
      "dataframe": "output->data_getxy->0"
    }
  },
  {
    "name": "data_featureselection",
    "module": "data_featureselection",
    "method": "SelectKBest",
    "transform": false,
    "input": {
      "X": "output->data_getxy->0",
      "Y": "output->data_getxy->1"
    },
    "options": {
      "score_func": "f_classif",
      "k": 9
    }
  },
  {
    "name": "model_build",
    "module": "model_build",
    "method": "mlp",
    "layers": [
        {"type": "input", "val": 20, "dim": 11, "init": "normal", "activation": "relu"},
        {"type": "dense", "val": 11, "init": "normal", "activation": "relu"},
        {"type": "output", "val": 1, "init": "normal"}
    ]
  },
  {
    "name": "model_train",
    "module": "model_train",
    "input":{
      "model": "output->model_build",
      "X": "output->data_featureselection->0",
      "Y": "output->data_featureselection->1"
    },
    "options":{
      "loss": "mse",
      "optimizer": "rmsprop",
      "scoring": ["mae", "accuracy"],
      "epoches": 200,
      "batch_size": 32,
      "validation_split": 0.33
    }
  }
]